<div class="container">

    <!--heading-->
    <div class="row">
        <div class="span12">
            <h3> Process Course</h3>
        </div>
    </div>


    <!--course ajax form-->
    <div class="row">
            <div class="span3">
                <input class="text_field" id="course_code" name="course_code" placeholder="Enter course code" type="text" />
            </div>

            <div class="span3">
                <input class="btn btn-primary" type="submit" value="Find course" />
            </div>
    </div>

    <!--course ajax table filler-->
    <div class="row">
        <div class="span12">
            <%= render 'course_ajax_filler' %>
        </div>
    </div>

    <!--the data input form-->
    <div class="row">
        <div class="span12">
            <%= render 'course_process_form' %>
        </div>
    </div>

    <div class="row">
        <div class="span12">
            <div style="border: 1px #f5f5f5 solid;margin:10px 0 10px 0"></div>
        </div>
    </div>

    <!--header-->
    <div class="row">
        <div class="span12">
            <h6>Table</h6>
        </div>
    </div>

    <!--list of student names-->
    <div class="row">
        <div class="span12">
            <h6>Table</h6>
        </div>
    </div>
</div>